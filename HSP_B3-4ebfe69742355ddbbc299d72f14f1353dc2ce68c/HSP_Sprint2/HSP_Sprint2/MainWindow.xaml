            <ComboBox HorizontalAlignment="Left" Margin="408,201,0,0" VerticalAlignment="Top" Width="120">
                <ComboBoxItem>Stahl</ComboBoxItem>
                <ComboBoxItem IsSelected="True">Titanium</ComboBoxItem>
                <ComboBoxItem>Aluminium</ComboBoxItem>
                <ComboBoxItem>kupfer</ComboBoxItem>
